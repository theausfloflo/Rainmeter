[Rainmeter]
Update=25
OnRefreshAction=[!ClickThrough 1][!KeepOnScreen 1]
ContextTitle=Show drag handle
ContextAction=[!ClickThrough 0][!KeepOnScreen 0][!ShowMeter MoveBG]

[Metadata]
Name=Snow
Author=killall-q.deviantart.com
Version=2014.06.05
Information=Frozen precipitation physics simulator. Activate drag handle in context menu to move to another monitor.
License=Creative Commons Attribution-Non-Commercial-Share Alike 3.0

[Variables]
; Vertical screen resolution / (Flakes * Rate) = terminal velocity.
Flakes=256
; Respawn a flake at the top of the screen every this many updates.
Rate=1
; Volatility of wind speed.
WindForce=1

; ----------------MEASURES--------------

[mScript]
Measure=Script
ScriptFile=#@#Snow.lua

; ----------------STYLES----------------

[sF1]
ImageName=#@#Images\Flake1.png
Hidden=1

[sF2]
ImageName=#@#Images\Flake2.png
Hidden=1

[sF3]
ImageName=#@#Images\Flake3.png
Hidden=1

[sF4]
ImageName=#@#Images\Flake4.png
Hidden=1

; ----------------METERS----------------

[AllocSpace]
Meter=String
W=#SCREENAREAWIDTH#
H=#SCREENAREAHEIGHT#

[MoveBG]
Meter=String
X=(#SCREENAREAWIDTH# / 2)
Y=(#SCREENAREAHEIGHT# / 2)
W=#SCREENAREAWIDTH#
H=#SCREENAREAHEIGHT#
FontFace=Segoe UI
FontSize=36
StringAlign=CenterCenter
FontColor=FFFFFF
SolidColor=000000A0
Text=Drag to move to another monitor.#CRLF#Double-click to close.
AntiAlias=1
Hidden=1
MouseActionCursor=0
LeftMouseDoubleClickAction=!Refresh

