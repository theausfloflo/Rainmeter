            [MeterButton1]
            Meter=BUTTON
            X=10
            Y=19
            ButtonImage="#@#images\Up3.png"
            ButtonCommand=[!SetVariable Modificator ((#Modificator#)-(([MeasureModificatorMinus])*86400))][!UpdateMeasure *][!UpdateMeter *][!Redraw]
            TooltipText=Previous Month
            DynamicVariables=1

            [MeterButton2]
            Meter=BUTTON
            X=204
            Y=r
            ButtonImage="#@#images\Up2.png"
            ButtonCommand=[!SetVariable Modificator ((#Modificator#)+(([MeasureModificator])*86400))][!UpdateMeasure *][!UpdateMeter *][!Redraw]
            TooltipText=Next Month
            DynamicVariables=1
            ;-------------------------------------------------------------------End
