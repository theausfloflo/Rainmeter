Calendar

            [Rainmeter]
            Update=1000
            BackgroundMode=1
            OnWakeAction=[!UpdateMeasure *][!UpdateMeter *][!Redraw]
            AccurateText=0
            ContextTitle3="-----"
            ContextTitle4="[MeasureWeekStartsMonday]#MenuWeekStartsMonday#"
            ContextAction4=[!WriteKeyValue Variables "WeekStartsMonday" "1" "#CURRENTFILE#"][!Refresh "#CURRENTCONFIG#"]
            ContextTitle5="[MeasureWeekStartsSunday]#MenuWeekStartsSunday#"
            ContextAction5=[!WriteKeyValue Variables "WeekStartsMonday" "0" "#CURRENTFILE#"][!Refresh "#CURRENTCONFIG#"]
            ContextTitle6="#ContextMenuConfigure# #ButtonSettingsGcal#"
            ContextAction6=["#@#Settings\calendar.txt"]
            ;-------------------------------------------------------------------
            [Metadata]
            Version=2.0
            Information=UI-Plano
            License=Bardweel
            Author=DesignByMichaels // Very THanks //
            ;-------------------------------------------------------------------
            [Variables]
            @Include=#@#System\Variables.inc
            @Include2=#@#System\Style.inc
            Modificator=0
            WeekStartsMonday=1
            ;-------------------------------------------------------------------
            [MeasureWeekStartsMonday]
            Measure=Calc
            Formula=1
            RegExpSubstitute=1
            Substitute="1":"#WeekStartsMonday#","^1$":"?   ","^0$":"?   "
            ;-------------------------------------------------------------------
            [MeasureWeekStartsSunday]
            Measure=Calc
            Formula=1
            RegExpSubstitute=1
            Substitute="1":"#WeekStartsMonday#","^0$":"?   ","^1$":"?   "
            ;-------------------------------------------------------------------
            ;-------------------------------------------------------------------
            [MeasureTime]
            Measure=Time


            [MeasureMonth]
            Measure=Time
            Format=%B %Y
            TimeStamp=([MeasureTime:])+(#Modificator#)
            DynamicVariables=1

            RegExpSubstitute=1

            [MeasureDay]
            Measure=Time
            Format=%A

            [MeasureDayOfMonth]
            Measure=Time
            Format=%d

            [MeasureModificator]
            Measure=Time
            Format=%b%Y
            TimeStamp=([MeasureTime:])+(#Modificator#)
            DynamicVariables=1

            RegExpSubstitute=1
            Substitute="1972":"Y","1976":"Y","1980":"Y","1984":"Y","1988":"Y","1992":"Y","1996":"Y","2000":"Y","2004":"Y","2008":"Y","2012":"Y","2016":"Y","2020":"Y","2024":"Y","2028":"Y","2032":"Y","2036":"Y","2040":"Y","2044":"Y","2048":"Y","2052":"Y","2056":"Y","2064":"Y","2068":"Y","2072":"Y","\d":"","Jan":"31","^Feb$":"28","FebY":"29","Mar":"31","Apr":"30","May":"31","Jun":"30","Jul":"31","Aug":"31","Sep":"30","Oct":"31","Nov":"30","Dec":"31","Y":""

            [MeasureModificatorMinus]
            Measure=Time
            Format=%b%Y
            TimeStamp=([MeasureTime:])+(#Modificator#)
            DynamicVariables=1

            RegExpSubstitute=1
            Substitute="1972":"Y","1976":"Y","1980":"Y","1984":"Y","1988":"Y","1992":"Y","1996":"Y","2000":"Y","2004":"Y","2008":"Y","2012":"Y","2016":"Y","2020":"Y","2024":"Y","2028":"Y","2032":"Y","2036":"Y","2040":"Y","2044":"Y","2048":"Y","2052":"Y","2056":"Y","2064":"Y","2068":"Y","2072":"Y","\d":"","Jan":"31","Feb":"31","MarY":"29","^Mar$":"28","Apr":"31","May":"30","Jun":"31","Jul":"30","Aug":"31","Sep":"31","Oct":"30","Nov":"31","Dec":"30","Y":""

            [MeasureMonthStarts]
            Measure=Time
            Format=%a%d
            TimeStamp=([MeasureTime:])+(#Modificator#)
            RegExpSubstitute=1
            Substitute="08":"01","09":"02","10":"03","11":"04","12":"05","13":"06","14":"07","15":"01","16":"02","17":"03","18":"04","19":"05","20":"06","21":"07","22":"01","23":"02","24":"03","25":"04","26":"05","27":"06","28":"07","29":"01","30":"02","31":"03","Mon01":"mo","Mon02":"so","Mon03":"sa","Mon04":"fr","Mon05":"do","Mon06":"mi","Mon07":"di","Tue01":"di","Tue02":"mo","Tue03":"so","Tue04":"sa","Tue05":"fr","Tue06":"do","Tue07":"mi","Wed01":"mi","Wed02":"di","Wed03":"mo","Wed04":"so","Wed05":"sa","Wed06":"fr","Wed07":"do","Thu01":"do","Thu02":"mi","Thu03":"di","Thu04":"mo","Thu05":"so","Thu06":"sa","Thu07":"fr","Fri01":"fr","Fri02":"do","Fri03":"mi","Fri04":"di","Fri05":"mo","Fri06":"so","Fri07":"sa","Sat01":"sa","Sat02":"fr","Sat03":"do","Sat04":"mi","Sat05":"di","Sat06":"mo","Sat07":"so","Sun01":"so","Sun02":"sa","Sun03":"fr","Sun04":"do","Sun05":"mi","Sun06":"di","Sun07":"mo","^(.+)$":"\1#WeekStartsMonday#","mo0":"di1","di0":"mi1","mi0":"do1","do0":"fr1","fr0":"sa1","sa0":"so1","so0":"mo1","1":"","mo":"7","di":"1","mi":"2","do":"3","fr":"4","sa":"5","so":"6"
            DynamicVariables=1


            [MeasureDateToday]
            Measure=Time
            Format=%#d
            TimeStamp=([MeasureTime:])+(#Modificator#)
            DynamicVariables=1


            [MeasureDateToday2]
            Measure=Time
            Format=%#d%b%Y
            TimeStamp=([MeasureTime:])
            DynamicVariables=1


            [MeasureDateA1]
            Measure=Time
            Format=%#d
            TimeStamp=([MeasureTime:])+(86400*0)-(86400*(([MeasureDateToday])-1))-(86400*([MeasureMonthStarts]))+(#Modificator#)
            DynamicVariables=1



            [MeasureDateA2]
            Measure=Time
            Format=%#d
            TimeStamp=([MeasureTime:])+(86400*1)-(86400*(([MeasureDateToday])-1))-(86400*([MeasureMonthStarts]))+(#Modificator#)
            DynamicVariables=1



            [MeasureDateA3]
            Measure=Time
            Format=%#d
            TimeStamp=([MeasureTime:])+(86400*2)-(86400*(([MeasureDateToday])-1))-(86400*([MeasureMonthStarts]))+(#Modificator#)
            DynamicVariables=1



            [MeasureDateA4]
            Measure=Time
            Format=%#d
            TimeStamp=([MeasureTime:])+(86400*3)-(86400*(([MeasureDateToday])-1))-(86400*([MeasureMonthStarts]))+(#Modificator#)
            DynamicVariables=1



            [MeasureDateA5]
            Measure=Time
            Format=%#d
            TimeStamp=([MeasureTime:])+(86400*4)-(86400*(([MeasureDateToday])-1))-(86400*([MeasureMonthStarts]))+(#Modificator#)
            DynamicVariables=1



            [MeasureDateA6]
            Measure=Time
            Format=%#d
            TimeStamp=([MeasureTime:])+(86400*5)-(86400*(([MeasureDateToday])-1))-(86400*([MeasureMonthStarts]))+(#Modificator#)
            DynamicVariables=1



            [MeasureDateA7]
