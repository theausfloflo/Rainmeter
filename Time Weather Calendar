            [MeasureToday]
            Measure=Plugin
            UpdateRate=30
            Plugin=Plugins\WebParser.dll
            Url=http://xml.weather.com/weather/local/#Location#?cc=*&unit=#Unit#&dayf=1
            RegExp="(?siU)<dnam>(.*),.*<tmp>(.*)</tmp>.*<t>(.*)</t>.*<icon>(.*)</icon>.*<day d="0" t="(.*)" dt=".*">.*<hi>(.*)</hi>.*<low>(.*)</low>.*<icon>.*<icon>(.*)</icon>"
            ;-----------------------------------------------------------------------------------------------------------
            [MeasureWeekDays]
